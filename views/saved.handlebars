{{#each articles}}
<div class="jumbotron jumbotron-fluid">

    <div class="container">
        <h6 id="title" class="display-4">{{this.title}}</h6>
        <a href={{this.link}}> Read It</a>
        <br>
    <button id="delete" class="btn btn-light" data-id="{{this.id}}">Delete</button>
        <br><br>
    <button id="addNote" class="btn btn-light" data-id="{{this.id}}">Add a Note</button> <br><br>
    <button id="viewNote" class="btn btn-light" data-id="{{this.id}}">View Notes</button>
    </div>
</div>
{{/each}} 
